# Environment Variables for Railway Deploy
# Copy this file and rename to .env in production

NODE_ENV=production
PORT=3000

# Wix App Settings
WIX_APP_ID=smart-product-grid-pro
ANALYTICS_ENABLED=true

# Optional - will be provided by Wix after app approval
# WIX_APP_SECRET=your-wix-app-secret-here
# WIX_APP_INSTANCE_SECRET=your-instance-secret-here

# Rate Limiting
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
